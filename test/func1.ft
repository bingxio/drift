def i: int = 1 // ONE PLUS

def (x: int) foo -> int
    if x == 8
        ret -1
    nf
        i += 1
        ret i * x
    end
end

def x: int = 0

for ->
    def sum: int = foo(x)

    sum

    out x == 10
    x += 1
end

